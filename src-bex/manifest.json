{
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "permissions": [
    "storage",
    "identity",
    "unlimitedStorage",
    "alarms",
    "offscreen",
    "notifications"
  ],
  "action": {
    "default_popup": "www/index.html#/popup"
  },
  "options_page": "www/index.html#/options",
  "background": {
    "service_worker": "background.js"
  }
}
